{
  "manifest_version": 3,
  "name": "Gradlife",
  "short_name": "gradlife",
  "version": "0.9.0",
  "description": "An extension that lets Pitt students submit (and discover) events for grad students on campus.",
  "host_permissions": ["https://us-central1-gradlife-505dc.cloudfunctions.net/*"],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html?#/popup",
    "default_title": "Gradlife"
  },
  "options_page": "index.html?#/options",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
